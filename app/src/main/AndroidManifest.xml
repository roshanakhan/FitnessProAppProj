<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.fitnessproapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".bmiActivity"></activity>
        <!--<activity android:name=".stepsActivity"></activity>-->
        <activity android:name=".caloriesActivity"></activity>
        <activity android:name=".all_timersActivity"></activity>
        <activity android:name=".tentimerActivity"></activity>
        <activity android:name=".thirtytimerActivity"></activity>
        <activity android:name=".sixtytimerActivity"></activity>
        <!--<activity android:name=".videosActivity"></activity>-->
        <!--<activity android:name=".musicActivity"></activity>-->
        <activity android:name=".feedbackActivity"></activity>
    </application>

</manifest>